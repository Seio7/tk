#content(data-router-wrapper)
  .router-view(data-router-view="{{ page.name | split: '.' | first }}")
    #blog-content
      .container
        .blog-hero-image
          h1 {{ page.title }}
        .col-2-container
          .main-col
            main
              | {{ content }}
          .side-col
            aside.main-sidebar
              include sidebar-blog
